{
    "Comment": "A Hello World example of the Amazon States Language using Pass states",
    "StartAt": "WaitForSomeTime",
    "States": {
        "WaitForSomeTime": {
            "Type": "Wait",
            "Next": "GetRandomMessage",
            "Comment": "Wait for a some time",
            "Seconds": 1
        },
        "GetRandomMessage": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:eu-north-1:392971033516:function:getRandomMessage",
            "TimeoutSeconds": 10,
            "Next": "Hello"
        },
        "Hello": {
            "Type": "Pass",
            "Result": "Hello",
            "Next": "World"
        },
        "World": {
            "Type": "Pass",
            "Result": "World",
            "End": true
        }
    }
}